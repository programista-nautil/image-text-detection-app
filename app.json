{
	"expo": {
		"name": "VisioAssist",
		"slug": "image-text-detection-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/ikona.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.daamian.imagetextdetectionapp"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/android-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.daamian.imagetextdetectionapp"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"extra": {
			"eas": {
				"projectId": "d2dd5ada-8a75-45b7-aea5-ef52abd98c64"
			}
		},
		"plugins": [
			[
				"react-native-vision-camera",
				{
					"cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera."
				}
			],
			[
				"expo-build-properties",
				{
					"android": {
						"minSdkVersion": 26,
						"compileSdkVersion": 34,
						"targetSdkVersion": 34
					}
				}
			]
		]
	}
}
